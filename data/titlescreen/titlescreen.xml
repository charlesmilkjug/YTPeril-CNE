<!DOCTYPE codename-titlestate >
<titlestate>
	<sprites folder="menus/titlescreen">
                <sprite name="bg" sprite="bgYouTube" x="0" y="0" />
		<sprite name="logo" sprite="logo" x="90" y="0" />
		<titleEnter sprite="titleEnter" x="-125" y="-100" />
	</sprites>

	<intro length="16">
		<text beat="1">
			<line text="charlescatyt" />
			<line text="theanimateman" />
